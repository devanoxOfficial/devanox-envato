{
    "name": "devanoxofficial/envato",
    "description": "Package for laravel. This package will help Devanox to manage application and checking licencnse key and update project.",
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devanox\\Envato\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Chetan",
            "email": "contact@mrchetan.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devanox\\Envato\\Providers\\DevanoxEnvatoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
